{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
      "strict": [2, "never"],
      "no-unused-vars": [2, {
        "vars": "local",
        "args": "after-used"
      }],
      "no-console": 0,
      "no-debugger": 2,
      "no-alert": 2,
      "key-spacing": [2, {
          "beforeColon": false,
          "afterColon": true
      }],
      "no-extra-semi": 2,
      "no-func-assign": 2,
      "no-invalid-regexp": 2,
      "no-dupe-keys": 2,
      "comma-dangle": [2, "never"]
    }
  }